{
  "name": "action-request",
  "version": "0.2.1",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "firebase:build": "yarn && ng build --aot --build-optimizer && yarn functions:build && yarn patch:ngsw",
    "firebase:build:production": "yarn && ng build --aot --build-optimizer --prod --service-worker && yarn functions:build && yarn patch:ngsw",
    "deploy": "yarn firebase:build && firebase deploy --project staging",
    "deploy:production": "yarn firebase:build:production && firebase deploy --project production",
    "functions:build": "cd functions && yarn firebase:build && cd ..",
    "analyze": "yarn && ng build --aot --stats-json && webpack-bundle-analyzer dist/stats.json",
    "patch:ngsw": "node ./tools/patch/ngsw-worker-patch.js"
  },
  "dependencies": {
    "@angular/animations": "5.2.10",
    "@angular/cdk": "5.2.5",
    "@angular/common": "5.2.10",
    "@angular/compiler": "5.2.10",
    "@angular/core": "5.2.10",
    "@angular/forms": "5.2.10",
    "@angular/http": "5.2.10",
    "@angular/material": "5.2.5",
    "@angular/platform-browser": "5.2.10",
    "@angular/platform-browser-dynamic": "5.2.10",
    "@angular/router": "5.2.10",
    "@angular/service-worker": "5.2.10",
    "angularfire2": "5.0.0-rc.7",
    "core-js": "2.5.6",
    "firebase": "4.13.1",
    "hammerjs": "2.0.8",
    "lodash": "4.17.19",
    "ngx-file-drop": "3.0.0",
    "object-hash": "1.3.0",
    "rxjs": "5.5.10",
    "xlsx": "0.12.10",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "1.7.4",
    "@angular/compiler-cli": "5.2.10",
    "@angular/language-service": "5.2.10",
    "@types/jasmine": "2.8.2",
    "@types/jasminewd2": "2.0.3",
    "@types/lodash": "4.14.107",
    "@types/node": "8.0.57",
    "@types/object-hash": "1.1.0",
    "codelyzer": "4.0.2",
    "firebase-admin":"5.12.0",
    "jasmine-core": "2.8.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "2.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.3.0",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "0.2.2",
    "protractor": "5.2.2",
    "ts-node": "3.3.0",
    "tslint": "5.8.0",
    "typescript": "2.5.3",
    "webpack-bundle-analyzer":"2.11.1"
  }
